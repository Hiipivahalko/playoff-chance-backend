      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      // TODO: Remove this once DSS updates menu links, this is needed in meantime b/c of redirects
      // Also used in analytics.js
      const getGtagPath = (path) => {
        // If we have "team" with no s at the end, add an s
        if (path.indexOf('team') !== -1 && path.indexOf('teams') === -1) {
          path = path.replace('team', 'teams');
        }
        if (path.indexOf('player') !== -1) {
          path = path.replace('player', 'skaters');
        }
        return path;
      }
      let path = getGtagPath(window.location.pathname);

      gtag('config', 'UA-80428426-3', {
        page_path: path,
        page_location: window.location.origin + path,
      });
    </script>

